[Unit]
Description=blackbox- Node exporter
After=network.target

[Service]
Type=simple
User=root
Group=root
ExecStart=/opt/blackbox/bin/blackbox_exporter-{{ vm_version }}.linux-{{ vm_arch }}/blackbox_exporter --config.file=/opt/blackbox/blackbox.yml --web.listen-address=:9115

[Install]
WantedBy=multi-user.target
